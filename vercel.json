{
  "version": 2,
  "functions": {
    "api/**/*.py": { "runtime": "python3.11" }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/", "dest": "/docs/index.html" },
    { "src": "/(.*)", "dest": "/docs/$1" }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/", "destination": "/docs/index.html" },
    { "source": "/(.*)", "destination": "/docs/$1" }
  ],
  "cleanUrls": true
}